{
  "manifest_version": 3,
  "name": "CX Boilerplate",
  "description": "Chrome Extension Boilerplate including RPC, API, React, Webpack, and more!",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["./styles/styles.css"],
      "js": ["content.js"]
    }
  ]
}
