{
    "manifest_version": 3,
    "name": "My Extension",
    "description": "Boilerplate",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "./scripts/background/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.google.com/*"
            ],
            "css": [
                "./styles/styles.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}